    "description": "Yii2 web project for personal use",
    "type": "project",
    "license": "GPL-3.0-only",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "component-dir": "../vendor-components",
        "vendor-dir": "../vendor",
        "platform": {
            "php": "7.3.6"
        }
    },
    "require": {
        "php": ">=7.3.0",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-bootstrap4": "~1.0.0",
        "yiisoft/yii2-httpclient": "~2.0.0",
        "yiisoft/yii2-jui": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.1.0",
        "bower-asset/fontawesome": "^5.0",
        "bower-asset/jqueryui-timepicker-addon": "^1.6",
        "bower-asset/to-markdown": "^3.0",
        "npm-asset/highlightjs": "^9.0",
        "npm-asset/lightbox2": "^2.10",
        "2amigos/yii2-usuario": "~1.0",
        "codemix/yii2-localeurls": "^1.7",
        "gkralik/php-uglifyjs": "^1.0",
        "himiklab/yii2-recaptcha-widget": "*",
        "kop/yii2-scroll-pager": "*",
        "mpdf/mpdf": "^8.0",
        "mpdf/qrcode": "^1.0",
        "voku/simple_html_dom": "^4.5",
        "zenorocha/clipboardjs": "^2.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
