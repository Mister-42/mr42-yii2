{
    "name": "thoulah/mr.42",
    "description": "Yii2 web project for personal use",
    "type": "project",
    "license": "GPL-3.0-only",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4.0",
        "yiisoft/yii2": "^2.0.36",
        "yiisoft/yii2-bootstrap4": "^2.0",
        "yiisoft/yii2-jui": "^2.0.0",
        "yiisoft/yii2-swiftmailer": "^2.1.0",
        "bower-asset/jqueryui-timepicker-addon": "^1.6",
        "npm-asset/bs-custom-file-input": "^1.3",
        "npm-asset/highlightjs": "^9.0",
        "npm-asset/lightbox2": "^2.0",
        "npm-asset/showdown": "^1.9",
        "2amigos/yii2-usuario": "~1.5.1",
        "gkralik/php-uglifyjs": "^1.0",
        "joshtronic/php-loremipsum": "^1.0",
        "kop/yii2-scroll-pager": "^2.0",
        "mpdf/mpdf": "^8.0",
        "mpdf/qrcode": "^1.0",
        "thoulah/yii2-fontawesome-inline": "1.1.x-dev",
        "thoulah/yii2-httpclient": "^1.0",
        "voku/simple_html_dom": "^4.7",
        "zenorocha/clipboardjs": "^2.0"
    },
    "require-dev": {
